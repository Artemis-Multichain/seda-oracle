{
  "routeGroup": "proxy",
  "routes": [
    {
      "path": "/",
      "method": ["GET", "OPTIONS", "POST"],
      "upstreamUrl": "https://api.etherscan.io/v2/api",
      "stripPrefix": false,
      "forwardPrefix": false,
      "responseHeaders": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      },
      "headers": {
        "x-api-key": "{$API_KEY}"
      }
    }
  ]
}
